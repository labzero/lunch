  "babel": {
    "presets": [
      "react",
      "node5",
      "stage-0"
    ],
    "env": {
      "test": {
        "plugins": [
          "rewire"
        ]
      }
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "globals": {
      "__DEV__": true
    },
    "env": {
      "browser": true
    },
    "rules": {
      "comma-dangle": 0,
      "key-spacing": 0,
      "no-confusing-arrow": 0,
      "jsx-quotes": [
        2,
        "prefer-double"
      ],
      "max-len": [
        2,
        120
      ],
      "react/jsx-no-bind": [
        2,
        {
          "ignoreRefs": true
        }
      ],
      "react/jsx-first-prop-new-line": 0,
      "react/require-render-return": 0,
      "jsx-a11y/aria-role": 0,
      "jsx-a11y/img-has-alt": 0,
      "jsx-a11y/img-redundant-alt": 0,
      "no-restricted-syntax": [2, "DebuggerStatement", "LabeledStatement", "WithStatement"],
      "no-underscore-dangle": 0,
      "no-param-reassign": 0,
      "global-require": 0,
      "import/no-unresolved": 0
    }
  },
  "jscsConfig": {
    "preset": "airbnb",
    "excludeFiles": [
      "build/**",
      "node_modules/**"
    ],
    "validateQuoteMarks": null,
    "disallowSpacesInsideTemplateStringPlaceholders": null,
    "maximumLineLength": 120,
    "requireCamelCaseOrUpperCaseIdentifiers": {
      "ignoreProperties": true
    },
    "requirePaddingNewLinesAfterBlocks": null,
    "requireTrailingComma": null
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "function-url-quotes": "single",
      "string-quotes": "single",
      "number-leading-zero": "never"
    }
  },
  "scripts": {
    "eslint": "eslint src tools",
    "jscs": "jscs src tools --verbose",
    "stylelint": "stylelint \"src/**/*.scss\" --syntax scss",
    "lint": "npm run eslint && npm run jscs && npm run stylelint",
    "test": "mocha src/**/**/*.test.js src/**/*.test.js --require test/setup.js --compilers js:babel-register",
    "test:watch": "mocha src/**/*.test.js src/**/**/*.test.js --require test/setup.js --compilers js:babel-register --reporter min --watch",
    "clean": "babel-node tools/run clean",
    "copy": "babel-node tools/run copy",
    "bundle": "babel-node tools/run bundle",
    "build": "babel-node tools/run build",
    "deploy": "babel-node tools/run deploy",
    "start": "babel-node tools/run start"
  }
}
