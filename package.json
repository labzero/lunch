  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --no-ignore --fix",
      "git add --force"
    ],
    "*.{json,md,graphql}": [
      "git add --force"
    ],
    "*.{css,less,styl,scss,sass,sss}": [
      "stylelint --fix",
      "git add --force"
    ]
  },
  "overrides": {
    "react": "npm:@preact/compat@*",
    "react-dom": "npm:@preact/compat@*"
  },
  "scripts": {
    "lint-js": "eslint --ignore-path .gitignore --ignore-pattern \"!**/.*\" .",
    "lint-css": "stylelint \"src/**/*.{css,less,styl,scss,sass,sss}\"",
    "lint": "bun run lint-js && bun run lint-css",
    "fix-js": "bun run lint-js -- --fix",
    "fix-css": "bun run lint-css -- --fix",
    "fix": "bun run fix-js && bun run fix-css",
    "test-file": "mocha",
    "test-file-ci": "npm run test-file --reporter mocha-junit-reporter",
    "test": "npm run test-file \"./src/**/*.test.ts\"",
    "test-ci": "npm run test-file-ci \"./src/**/*.test.ts\"",
    "test-watch": "npm run test --watch --notify",
    "test-cover": "nyc npm run test",
    "coverage": "npm run test-cover && open-cli coverage/lcov-report/index.html",
    "db:seed:all": "bun ./db/seed.ts",
    "db:migrate": "bun ./db/migrate.ts",
    "db:migrate:undo": "bun ./db/undo.ts",
    "db:migrate:undo:all": "bun ./db/undo-all.ts",
    "debug": "bun run start -- --inspect",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run-file": "cypress run --spec",
    "cypress:run-integration": "cypress run --config baseUrl=http://local.lunch.pink:3000/ --env subdomain=http://integration-test.local.lunch.pink:3000/",
    "integration-serve": "NODE_ENV=test bun ./tools/run runServer || true",
    "integration-setup": "npm run db:migrate:undo:all && npm run db:migrate && npm run db:seed:all && npm run build",
    "integration": "NODE_ENV=test npm run integration-setup && (npm run integration-serve & npm run cypress:run)",
    "integration-file": "NODE_ENV=test npm run integration-setup && (npm run integration-serve & npm run \"cypress:run-file {1}\" --)",
    "integration-interactive": "NODE_ENV=test npm run integration-setup && (npm run integration-serve & npm run cypress:open)",
    "integration-test-ci": "NODE_ENV=test npm run integration-setup && (npm run integration-serve & npm run cypress:run-integration)",
    "clean": "bun ./tools/run clean",
    "copy": "bun ./tools/run copy",
    "bundle": "bun ./tools/run bundle",
    "build": "bun ./tools/run build",
    "build-stats": "npm run build -- --release --analyse",
    "deploy": "bun ./tools/run deploy",
    "render": "bun ./tools/run render",
    "start": "bun ./tools/run start",
    "prepare": "husky install"
  },
  "packageManager": "yarn@3.5.1"
}
